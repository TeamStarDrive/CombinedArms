  <PowerDraw>32</PowerDraw>
  <BonusRepairRate>400</BonusRepairRate>

  <SensorRange>33000</SensorRange>
  <TargetTracking>10</TargetTracking>


  <NameIndex>920</NameIndex>
  <DescriptionIndex>921</DescriptionIndex>

  <MechanicalBoardingDefense>50</MechanicalBoardingDefense>


  <!-- for shield items -->
  <shield_power>0</shield_power>
  <shield_power_max>0</shield_power_max>
  <shield_recharge_delay>0</shield_recharge_delay>
  <shield_recharge_rate>0</shield_recharge_rate>
  <shield_radius>0</shield_radius>
  <AmplifyShields>8000</AmplifyShields>
  <!---repair and defence-->
  <Regenerate>80</Regenerate>
  <EMP_Protection>2000</EMP_Protection>
  <InhibitionRadius>10000</InhibitionRadius>
  <!-- for PowerPlant Items -->
  <explodes>false</explodes>
